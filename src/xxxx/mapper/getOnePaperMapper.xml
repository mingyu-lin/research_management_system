<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="xxxx.mapper.getOnePaperMapper">
    <!--命名空间-->
    <select id="getOnePaper" parameterType="int" resultType="xxxx.entity.Paper">
        select * from paper where paper.id=#{id}
    </select>

</mapper>